class Solution {
    public static boolean checkEqual(int[] a, int[] b) {
        // code here
        int m = a.length;
        int n = b.length;
        Arrays.sort(a);
        Arrays.sort(b);
        if(m != n){
            return false;
        }
        else{
            for(int i=0;i<m;i++){
                if(a[i] != b[i]){
                    return false;
                }
            }
        }
        return true;
    }
}
